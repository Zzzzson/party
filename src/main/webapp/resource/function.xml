<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springapp.dao.FunctionMapper">
    <select id="getFunction" resultType="Function">
        SELECT * FROM FUNCTION WHERE FUNCITON_ID = #{id}
    </select>

    <insert id="insertFunction" parameterType="Function">
        INSERT INTO FUNCTION (NAME,INTRODUCTION,ADDRESS) VALUE (#{name},#{introduction},#{address})
    </insert>

    <update id="update" parameterType="Function">

    </update>
</mapper>